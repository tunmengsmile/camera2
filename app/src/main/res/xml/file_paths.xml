<?xml version="1.0" encoding="utf-8"?>
<paths>
    <!--path：需要临时授权访问的路径（.代表所有路径）-->
    <!--name：就是你给这个访问路径起个名字-->
    <external-path name="external-path" path="CameraDemo" />

    <!--外部存储-->
    <external-path
        name="AppInstaller"
        path="/Download"></external-path>

    <!--motion图片数据存储再外部存储DCIM文件夹中-->
    <external-path
        name="motion"
        path="/DCIM"></external-path>
</paths>